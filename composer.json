  },
  "require-dev": {
    "amphp/phpunit-util": "v3.0.0-beta.3",
    "cspray/labrador-coding-standard": "0.2.0",
    "phpunit/phpunit": "^9.5",
    "rdlowrey/auryn": "^1.4",
    "psalm/phar": "^4.26"
  },
  "autoload": {
    "psr-4": {
      "Labrador\\AsyncEvent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Labrador\\AsyncEvent\\Test\\": "test/"
    }
  }
