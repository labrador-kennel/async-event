{
  "name": "labrador-kennel/async-event",
  "description": "Trigger semantic application events powered by Amp Event Loop",
  "keywords": ["amphp", "event-emitter", "labrador-kennel", "annotated-container"],
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Sprayberry",
      "email": "contact@labrador-kennel.io",
      "homepage": "https://labrador-kennel.io",
      "role": "Project Maintainer"
    }
  ],
  "scripts": {
    "code-lint": "vendor/bin/labrador-cs src/ test/",
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": "^8.1",
    "amphp/amp": "~v3.0",
    "labrador-kennel/composite-future": "^1.2"
  },
  "require-dev": {
    "amphp/phpunit-util": "~v3.0",
    "cspray/labrador-coding-standard": "0.2.0",
    "phpunit/phpunit": "^9.5",
    "psalm/phar": "^5.6",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Labrador\\AsyncEvent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Labrador\\AsyncEvent\\Test\\": "test/"
    }
  },
  "extra": {
    "$annotatedContainer": {
      "initializers": [
        "Labrador\\AsyncEvent\\DependencyInjection\\AsyncEventInitializer"
      ]
    }
  }
}
