{
  "name": "cspray/labrador-async-event",
  "description": "Trigger semantic application events powered by Amp Event Loop",
  "keywords": ["amphp", "event", "emitter", "labrador-kennel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Sprayberry",
      "email": "contact@labrador-kennel.io",
      "homepage": "https://labrador-kennel.io",
      "role": "Project Maintainer"
    }
  ],
  "scripts": {
    "code-lint": "vendor/bin/labrador-cs src/ test/",
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": "^8.1",
    "amphp/amp": "~v3.0.0-beta.4",
    "cspray/annotated-container": "^1.5",
    "cspray/labrador-composite-future": "^1.1",
    "cspray/labrador-exceptions": "^1.2"
  },
  "require-dev": {
    "amphp/phpunit-util": "v3.0.0-beta.3",
    "cspray/labrador-coding-standard": "0.2.0",
    "phpunit/phpunit": "^9.5",
    "rdlowrey/auryn": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Labrador\\AsyncEvent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Labrador\\AsyncEvent\\Test\\": "test/"
    }
  }
}
